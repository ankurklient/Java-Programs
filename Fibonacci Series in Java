import java.util.*;
public class Main
{
	public static void main(String[] args) {
		System.out.print("Enter the number of digits for the fibonacci series : ");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int n1=0,n2=1,n3=0;
		while(n>0)
		{
		    System.out.print(n3+" ");
		    n1=n2;
		    n2=n3;
            n3=n1+n2;
		    n--;
		}
	}
}
