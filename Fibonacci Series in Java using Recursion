import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
		System.out.print("Enter the number of digits for the fibonacci series : ");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		fibonacci(n);
	}
	static int n1=0,n2=1,n3=0;
	public static void fibonacci(int n)
	{
		if(n>0)
		{
			System.out.print(n3+" ");
			n1=n2;
			n2=n3;
			n3=n1+n2;
			fibonacci(n-1);
		}
	}
}
