import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    System.out.print("Enter a number : ");
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    print(n);
	}
	
	public static void print(int n){
	    if(n>0)
	    {   
          System.out.println(n);
	        print(n-1);
	    }
	}
}
