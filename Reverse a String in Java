import java.util.*;
import java.lang.String;
public class Main
{
	public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      String s=sc.nextLine();
      int l=s.length();
      char st[]=s.toCharArray();
      char str[]=new char[l];
      for(int i=0;i<l;i++)
          str[i]=st[l-i-1];
      System.out.print(str);
  }
}
